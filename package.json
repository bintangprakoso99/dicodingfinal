{
  "name": "dicoding-stories-spa",
  "version": "1.0.0",
  "description": "A Single Page Application for sharing stories in the Dicoding community",
  "main": "scripts/main.js",
  "scripts": {
    "dev": "webpack serve --config webpack.config.cjs --mode development --open",
    "dev:port": "webpack serve --config webpack.config.cjs --mode development --port 3001 --open",
    "build": "webpack --config webpack.config.cjs --mode production",
    "build:dev": "webpack --config webpack.config.cjs --mode development",
    "start": "webpack serve --config webpack.config.cjs --mode development",
    "start:3001": "webpack serve --config webpack.config.cjs --mode development --port 3001",
    "start:8080": "webpack serve --config webpack.config.cjs --mode development --port 8080",
    "kill-port": "npx kill-port 9000 3000 3001 8080",
    "lint": "eslint --config eslint.config.cjs scripts/**/*.js",
    "lint:fix": "eslint --config eslint.config.cjs scripts/**/*.js --fix",
    "format": "prettier --write \"**/*.{js,css,html,json}\"",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --config jest.config.cjs --watch",
    "serve": "http-server dist -p 8080 -c-1",
    "clean": "rimraf dist",
    "analyze": "webpack-bundle-analyzer dist/bundle.js",
    "audit-fix": "npm audit fix",
    "check-config": "node -e \"console.log('Babel config:', require('./babel.config.cjs')); console.log('Webpack config type:', typeof require('./webpack.config.cjs'));\""
  },
  "keywords": ["dicoding", "stories", "spa", "pwa", "javascript", "webpack", "leaflet", "camera", "geolocation"],
  "author": "Dicoding Student",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/plugin-transform-class-properties": "^7.24.0",
    "@babel/plugin-transform-private-methods": "^7.24.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "@eslint/js": "^9.15.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@testing-library/jest-dom": "^6.4.2",
    "mini-css-extract-plugin": "^2.9.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "style-loader": "^4.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.95.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "workbox-webpack-plugin": "^7.1.0",
    "core-js": "^3.38.1",
    "kill-port": "^2.0.1"
  },
  "dependencies": {
    "leaflet": "^1.9.4"
  },
  "browserslist": ["> 1%", "last 2 versions", "not dead"],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
